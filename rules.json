      "enabled": true,
      "category": "fluff",
      "priority": 5,
      "patterns": [
        { "find": "^(hey|hi|hello|greetings|howdy|yo|sup|what's up|good morning|good afternoon|good evening)( there)?[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bhope you'?r?e? (having|doing|feeling)[\\w\\s]*[!.]\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bhope (this finds you well|all is well|you'?r?e? well|everything is going well)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bhave a (great|wonderful|nice|good|lovely|fantastic|amazing) (day|one|evening|night)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bthanks[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bthank you[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bthanks[!.,]?\\s+", "flags": "gi", "replace": "" },
        { "find": "\\bthank you[!.,]?\\s+", "flags": "gi", "replace": "" },
        { "find": "\\bthanks (again|so much|a lot|in advance|a bunch|a ton|for that|for this|for everything|for your help|for your time|for your assistance|for helping|a million|heaps|muchly)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bthank you (so much|very much|again|in advance|for your time|for your help|for your assistance|for that|for this|for everything|kindly|ever so much)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bthanks?,?\\s*-?\\s*[A-Z][a-z]+$", "flags": "gm", "replace": "" },
        { "find": "\\bmany thanks[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bthx[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bty[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bthanking you[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bmuch appreciated[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bI appreciate (it|that|this|your help|your time|your assistance)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bappreciate (it|that|this|your help|your time)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bgreatly appreciated[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bin advance for\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bin advance[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bfor your help[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bfor that[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bfor this[!.,]?\\s*$", "flags": "gim", "replace": "" },
        { "find": "\\bsorry (for|about) the (long|lengthy|wordy) (message|email|text|prompt)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bno pressure( though| at all)?[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bonly if you (want|have time|can|don't mind)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\blet me know what you think[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bwhen(ever)? you get a chance[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bhope (that makes sense|this helps|I explained it well)[!.,]?\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bdoes that make sense\\??\\s*", "flags": "gi", "replace": "" },
        { "find": "\\btake your time[!.,]?\\s*", "flags": "gi", "replace": "" }
      ]
    },
    {
      "id": "remove-please",
      "name": "Remove Pleasantries",
      "description": "Removes \"please\" from requests",
      "explanation": "AI models respond equally well without \"please\" - saves tokens.",
      "example": { "before": "Please write a poem", "after": "Write a poem" },
      "enabled": true,
      "category": "fluff",
      "priority": 10,
      "patterns": [
        { "find": "\\bplease\\s+", "flags": "gi", "replace": "" },
        { "find": ",?\\s*please[!.,]?$", "flags": "gi", "replace": "" },
        { "find": "\\bpretty please\\s*", "flags": "gi", "replace": "" },
        { "find": "\\bif you please\\s*", "flags": "gi", "replace": "" }
